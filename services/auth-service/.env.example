# auth-service/.env.example
# Replace example values with real secrets before use.

# Active Spring profile
ACTIVE_PROFILE=local

# PostgreSQL (Auth DB)
AUTH_SERVICE_DB_URL=jdbc:postgresql://localhost:5432/authdb
AUTH_SERVICE_DB_USER=auth_user
AUTH_SERVICE_DB_PASSWORD=change_me

# MongoDB (Audit log)
AUDIT_LOG_DB_URL=mongodb://localhost:27017/auditdb
MONGO_AUTH_DB=admin

# JWT configuration
JWT_SECRET=change_this_secret_key
JWT_ACCESS_EXPIRATION=expiration-in-millisconds
JWT_REFRESH_EXPIRATION=refresh-expiration-in-milliseconds

# Eureka
EUREKA_URL=localhost:8761

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# AWS / SQS
AWS_REGION=us-east-1
AWS_ACCESS_KEY=AKIAEXAMPLEACCESSKEY
AWS_SECRET_KEY=exampleSecretKeyHere
SQS_ENDPOINT=http://localhost:4566
USER_LOGIN_QUEUE=https://sqs.us-east-1.amazonaws.com/123456789012/user-login-queue
USER_REGISTRATION_QUEUE=https://sqs.us-east-1.amazonaws.com/123456789012/user-registration-queue
PASSWORD_RESET_QUEUE=